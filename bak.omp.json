{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#ffe9aa",
          "foreground": "#100e23"
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#ffffff",
          "foreground": "#100e23",
          "template": "{{ if .SSHSession }}\ueba9 {{ end }}{{ if eq .UserName .Env.POSH_USERNAME }} \uf007 {{else}}{{.UserName}}{{end}}@{{ if eq .HostName .Env.POSH_HOSTNAME }} \uea7a {{else}}{{.HostName}}{{end}}"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#91ddff",
          "foreground": "#100e23",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 3,
            "mapped_locations": {
              "D:\\Workspace": "\udb80\ude0f ",
              "D:\\Packages": "\ueb29 "
            }
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#95ffa4",
          "foreground": "#193549",
          "properties": {
            "fetch_status": true,
            "branch_max_length": 24,
            "mapped_branches": {
              "feat/*": " \uf135 ",
              "feature/*": " \uf135 ",
              "bug/*": " \uf188 ",
              "bugfix/*": " \uf188 "
            }
          }
        },
        {
          "type": "status",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "background": "#ff8080",
          "foreground": "#ffffff",
          "template": " \udb80\uded4 "
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
